{
  "name": "inline-css-to-file-extractor",
  "displayName": "InlineCssToFile",
  "description": "Move inline styles to a file.",
  "version": "1.0.0",
  "pricing": "Free",
  "license": "MIT",
  "publisher": "NeuSandeZ",
  "author": {
    "name": "Wojciech Massalski",
    "url": "https://github.com/NeuSandeZ"
  },
  "repository": {
    "type": "github",
    "url": "https://github.com/NeuSandeZ/InlineCssToFileExtension"
  },
  "categories": [
    "Programming Languages"
  ],
  "keywords": [
    "css",
    "html",
    "extractor",
    "inline style",
    "inline css",
    "style to file",
    "javascript",
    "typescript",
    "angular",
    "vue",
    "react"
  ],
  "activationEvents": [
    "onLanguage:html"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.extractInlineCssToNewFile",
        "title": "Move to new file"
      },
      {
        "command": "extension.extractInlineCssToFile",
        "title": "Move to file"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "engines": {
    "vscode": "^1.88.0"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@types/vscode": "^1.88.0",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "@vscode/test-cli": "^0.0.8",
    "@vscode/test-electron": "^2.3.9",
    "eslint": "^8.57.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "htmlparser2": "^9.1.0"
  }
}
